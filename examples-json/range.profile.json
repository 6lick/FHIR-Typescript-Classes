            "key": "rng-2",
            "severity": "error",
            "human": "If present, low SHALL have a lower value than high",
            "xpath": "not(exists(f:low/f:value/@value)) or not(exists(f:high/f:value/@value)) or (number(f:low/f:value/@value) <= number(f:high/f:value/@value))"
